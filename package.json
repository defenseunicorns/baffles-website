{
  "name": "baffles.dev",
  "version": "1.0.0",
  "private": true,
  "description": "baffles.dev",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "precommit": "husky",
    "build": "gatsby build",
    "build:staging": "npm run build && echo 'User-agent: *\nDisallow: /' > public/robots.txt",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "start": "npm run build && npm run serve",
    "dev": "npm run clean && gatsby develop",
    "typecheck": "tsc --noEmit",
    "preinstall": "npx npm-force-resolutions",
    "lint:fix": "npm run lint --fix",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx,.jsx .",
    "prettier:check": "prettier --ignore-path .gitignore --check \"./src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "format": "prettier --ignore-path .gitignore --write \"./src/**/*.{js,jsx,ts,tsx,json,md}\" .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "11.9.3",
    "@emotion/server": "11.4.0",
    "@emotion/styled": "11.9.3",
    "@fontsource/m-plus-rounded-1c": "4.5.8",
    "@fontsource/roboto": "4.5.7",
    "@mui/icons-material": "5.8.4",
    "@mui/material": "5.8.7",
    "gatsby": "4.15.2",
    "gatsby-plugin-advanced-sitemap": "2.1.0",
    "gatsby-plugin-google-tagmanager": "4.18.0",
    "gatsby-plugin-image": "2.18.0",
    "gatsby-plugin-manifest": "4.18.0",
    "gatsby-plugin-react-helmet": "5.18.0",
    "gatsby-plugin-react-svg": "3.1.0",
    "gatsby-plugin-sharp": "4.18.0",
    "gatsby-plugin-sitemap": "5.18.0",
    "gatsby-source-filesystem": "4.18.0",
    "gatsby-transformer-sharp": "4.18.0",
    "octokit": "1.8.1",
    "query-string": "7.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0"
  },
  "devDependencies": {
    "@octokit/types": "6.39.0",
    "@types/node": "17.0.45",
    "@types/react": "17.0.49",
    "@types/react-dom": "17.0.17",
    "@types/react-helmet": "6.1.5",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "gatsby-plugin-eslint": "4.0.2",
    "husky": "7.0.1",
    "lint-staged": "12.5.0",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  },
  "resolutions": {
    "ws": "7.4.6",
    "immer": "9.0.6",
    "ansi-html": "0.0.9",
    "nth-check": "2.0.1",
    "node-fetch": "2.6.7",
    "ansi-regex": "5.0.1",
    "browserslist": "4.16.5",
    "async": "3.2.3",
    "cross-fetch": "3.1.5",
    "jpeg-js": "0.4.4",
    "busboy": "1.6.0",
    "got": "11.8.5",
    "moment": "2.29.4",
    "parse-path": "5.0.0",
    "multer": "1.4.5-lts.1"
  }
}
